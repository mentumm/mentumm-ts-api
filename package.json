{
  "name": "staples-ts-api",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:denvermullets/staples-ts-api.git",
  "author": "denvermullets <devmullets@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn install && tsc",
    "start:prod": "yarn db:migrate && node build/index.js",
    "start": "nodemon ./src/index.ts",
    "knex": "./node_modules/.bin/knex --knexfile src/database/knexfile.ts",
    "db:migrate": "yarn knex migrate:latest src/database/knexfile.ts",
    "db:rollback": "yarn knex migrate:rollback src/database/knexfile.ts"
  },
  "engines": {
    "node": ">=16.14.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.23",
    "nodemon": "^2.0.15",
    "semantic-release": "^19.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "knex": "^1.0.7",
    "mixpanel": "^0.16.0",
    "moment": "^2.29.3",
    "pg": "^8.7.3"
  }
}
